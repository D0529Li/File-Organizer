<Window x:Class="File_Organizer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="clr-namespace:File_Organizer"
        Title="File Organizer" Height="600" Width="1000">
    <Grid Background="Pink">
        <TabControl>
            <TabItem Header="Organizer">
                <d:OrganizerUserControl />
            </TabItem>
            <TabItem Header="Selector">
                <d:SelectorUserControl />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
